{
	"compilerOptions": {
		"esModuleInterop": true,
		"strict": true,
		"jsx": "react-jsx",
		"sourceMap": true,
		"skipLibCheck": true,
		"resolveJsonModule": true,
		"forceConsistentCasingInFileNames": true,
		"checkJs": true,
		"allowJs": true,
		"jsxImportSource": "hono/jsx",
		"moduleResolution": "bundler",
		"module": "esnext",
		"types": ["bun-types"],
		"outDir": "dist",
		"baseUrl": ".",
		"paths": {
			"$lib/*": ["src/lib/*"]
		}
	}
}
